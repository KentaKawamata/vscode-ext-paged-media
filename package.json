{
	"name": "vscode-ext-paged-media",
	"description": "W3C Paged Media Viewer",
	"version": "0.0.1",
	"publisher": "abechanta",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:pagedMedia.open",
		"onWebviewPanel:pagedMedia"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pagedMedia.open",
				"title": "Open Paged Media Viewer",
				"category": "Paged Media"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.32.0"
	}
}
